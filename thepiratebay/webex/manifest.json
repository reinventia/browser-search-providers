{
	"manifest_version": 2,
	"version": "20.2",
	"default_locale": "en",
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"short_name": "Pirate Bay Search (by Seeders)",
	"author": "Reinventia - Desarrollo Web/Apps Madrid",
	"homepage_url": "https://madrid.reinventia.net",
	"applications": {
    	"gecko": {
      		"strict_min_version": "65.0"
    	}
    },
	"icons": {
		"48": "icons/tpbx48.png",
		"64": "icons/tpbx64.png",
		"96": "icons/tpbx96.png",
		"128": "icons/tpbx128.png"
	  },
	"chrome_settings_overrides": {
		"search_provider": {
			"name": "Pirate Bay Search (by Seeders)",
			"search_url": "https://thepiratebay.org/search/{searchTerms}/0/7/0",
			"favicon_url": "https://thepiratebay.org/favicon.ico",
			"keyword": "torrent",
			"is_default": false,
			"encoding": "UTF-8"
		}
	}
}
